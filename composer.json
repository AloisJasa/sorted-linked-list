{
	"name": "aloisjasa/sorted-linked-list",
	"description": "Sorted linked list",
	"authors": [
		{
			"name": "Alois Ja≈°a",
			"email": "aloisjasa@gmail.com"
		}
	],
	"prefer-stable": true,
	"require": {
		"php": "^8.3"
	},
	"require-dev": {
		"phpstan/phpstan": "^2.1",
		"phpstan/phpstan-strict-rules": "^2.0",
		"slevomat/coding-standard": "^8.15",
		"phpunit/phpunit": "^10.5",
		"shipmonk/phpstan-rules": "^4.1"
	},
	"autoload": {
		"psr-4": {
			"AloisJasa\\SortedLinkedList\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"AloisJasa\\SortedLinkedList\\": "tests/Unit"
		}
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		},
		"sort-packages": true
	}
}
